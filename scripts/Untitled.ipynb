   "source": [
    "init_lr = 5e-4\n",
    "max_steps = 1000\n",
    "exponential_decay_scheduler = optax.exponential_decay(\n",
    "    init_value=init_lr,\n",
    "    transition_steps=max_steps,\n",
    "    decay_rate=0.1)\n",
    "tx = optax.adam(learning_rate=exponential_decay_scheduler)"
